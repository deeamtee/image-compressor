{
  "name": "reduce-image-size-extension",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "prepare": "husky install",
    "format": "prettier --write .",
    "preview": "vite preview"
  },
  "husky": {
