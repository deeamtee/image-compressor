{
    "manifest_version": 3,
    "name": "Reduce Image Size",
    "version": "1.0",
    "description": "Chrome Extension built with React and TypeScript",
    "action": {
      "default_popup": "index.html"
    },
    "icons": {
      "32": "coffee32.svg"
    },
    "permissions": ["scripting"],
    "host_permissions": ["<all_urls>"]
  }
  