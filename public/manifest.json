{
    "manifest_version": 3,
    "name": "React Chrome Extension",
    "version": "1.0",
    "description": "Chrome Extension built with React and TypeScript",
    "action": {
      "default_popup": "index.html",
      "icons": {
        "16": "/coffee.svg",
        "48": "/coffee.svg",
        "128": "/coffee.svg"
      }
    },
    "permissions": ["storage"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["assets/content.js"],
        "css": ["assets/content.css"]
      }
    ]
  }
  